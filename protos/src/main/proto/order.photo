syntax = "proto3"; //proto3을 사용함

option java_multiple_files = true;
option java_package = "me.hanwook.order.service";
option java_outer_classname = "OrderService";

service OrderService {
  rpc create (OrderRequest) returns (OrderResponse) {
  }
}

message OrderRequest {
    int64 userId;
    stirng name;
    int32 quantity;
    int32 price;
}

message OrderResponse {
    int64 userId;
    string name;
    int32 quantity;
    int32 price;
}